		margin-bottom: -14px;
	}

script.
	loadjs.ready('PRELoadB', function() {
		console.log('form')

		loadjs('/zmodel/setupModel.js')
		loadjs.ready('ModelB', function() { // bundle
			setupPg()
		})

		loadjs(['/assets/gridforms/gridforms.css'
				,'/assets/gridforms/gridforms.js'])
	})//ready
	
	
	function setupPg() {
		console.log('setup')

		var email = User.emailGet()
		$('#email').text(email)

		//get name
		var name
		User.getUserAttributes().then(function(bean){
			console.log(bean)
			name = bean['name']
			console.log(name)
			$('#name').text(name)
		}).catch(function(err) {
			console.log('Bean Auth Error:')
			console.log(err)
		})

		$( '#resetB' ).click(function(e) {
			e.preventDefault()
			User.requestPswdReset(email)
			window.location.href = '/register/resetPswd'
		})// on click

	}//setup()


